@model IEnumerable<TeHagoLaFila.Models.Reservacion>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.InitialTime)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EndTime)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InitialTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndTime)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ReservacionID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ReservacionID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ReservacionID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
