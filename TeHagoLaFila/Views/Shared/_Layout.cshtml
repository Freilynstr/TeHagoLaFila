<!DOCTYPE html>

<html>

<head>
    <meta charset="utf-8" />
    <link rel="icon" sizes="76x76" href="~/images/icono.jpg">
    <link rel="icon" type="image" href="~/images/icono.jpg">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>@ViewData["Title"] - TeHagoLaFila</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />

    <!-- CSS Files -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/light-bootstrap-dashboard.css?v=2.0.1" rel="stylesheet" />

    <!--     Fonts and icons     -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    <link href="http://netdna.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet">

    <environment include="Development">

    </environment>
    <environment exclude="Development">

    </environment>
</head>

<body>
    <div class="wrapper">
        <div class="sidebar" data-color="blue" data-image="/images/sidebar-4.jpg">
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a class="simple-text" asp-area="" asp-controller="Home" asp-action="Index" title="Home">
                        <img src="~/images/logo.jpg" class="rounded" width="160" height="25"/>
                    </a>
                </div>

                <ul class="nav">
                    <li class="nav-item active">
                        <a class="nav-link" asp-area="" asp-controller="CategoriaNegocio" asp-action="Index" title="Reservar un turno">
                            <i class="nc-icon nc-notes"></i>
                            <p>Reservar turno</p>
                        </a>
                    </li>
                    <br />
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index" title="Dashboard">
                            <i class="nc-icon nc-bulb-63"></i>
                            <p>DashBoard</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" asp-area="" asp-controller="Manage" asp-action="Index" title="Profile">
                            <i class="nc-icon nc-circle-09"></i>
                            <p>User Profile</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="#Favoritos">
                            <i class="nc-icon nc-preferences-circle-rotate"></i>
                            <p>Mis Negocios</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="#Maps">
                            <i class="nc-icon nc-circle-09"></i>
                            <p>Mis Empleados</p>
                        </a>
                    </li>
                    <li class="nav-item active active-pro">
                        <a class="nav-link active" asp-action="Create" asp-controller="Negocio">
                            <i class="nc-icon nc-simple-add"></i>
                            <p>Registrar Negocio</p>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg navbar-static-top" color-on-scroll="500">
                <div class=" container-fluid  ">
                    <a class="navbar-brand" href="#Search">Search <i class="nc-icon nc-zoom-split"></i></a>

                    <button href="" class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-bar burger-lines"></span>
                        <span class="navbar-toggler-bar burger-lines"></span>
                        <span class="navbar-toggler-bar burger-lines"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-end" id="navigation">
                        <ul class="navbar-nav ml-auto">
                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index" title="Home">
                                    <span class="no-icon">Home</span>
                                </a>
                            </li>
                            <li class="nav-item">
                                @await Html.PartialAsync("_LoginPartial")
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <!-- End Navbar -->

            <!--Body-->
            <div class="content">
                <div class="container-fluid">
                    <div class="section">
                        @RenderBody()
                    </div>
                </div>
            </div>
            <!--End Body-->

            <!--Footer-->
            <footer class="footer">
                <div class="container">
                    <nav>
                        <ul class="footer-menu">
                            <li>
                                <a href="#">
                                    Home
                                </a>
                            </li>
                        </ul>
                        <p class="copyright text-center">
                            ©
                            <script>
                                document.write(new Date().getFullYear())
                            </script>
                            <a href="http://www.github.com/Freilynstr">Freilyn Tavárez</a> for my Programming Project
                        </p>
                    </nav>
                </div>
            </footer>
            <!--End Footer-->
        </div>
    </div>

    <environment include="Development">
        <!--   Core JS Files   -->
        <script src="https://google-code-prettify.googlecode.com/svn/loader/run_prettify.js"></script>
        <script src="~/js/core/jquery.3.2.1.min.js" type="text/javascript"></script>
        <script src="~/js/core/popper.min.js" type="text/javascript"></script>
        <script src="~/js/core/bootstrap.min.js" type="text/javascript"></script>
        <!--  Plugin for Switches, full documentation here: http://www.jque.re/plugins/version3/bootstrap.switch/ -->
        <script src="~/js/plugins/bootstrap-switch.js"></script>
        <!--  Google Maps Plugin    -->
        <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyALc7eQ_lc33X22RDqTD1l4-sAMRqo2DSY"></script>
        <!--  Chartist Plugin  -->
        <script src="~/js/plugins/chartist.min.js"></script>
        <!--  Notifications Plugin    -->
        <script src="~/js/plugins/bootstrap-notify.js"></script>
        <!-- Control Center for Light Bootstrap Dashboard: scripts for the example pages etc -->
        <script src="~/js/light-bootstrap-dashboard.js?v=2.0.1" type="text/javascript"></script>

        
    </environment>
    <environment exclude="Development">
        
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>
